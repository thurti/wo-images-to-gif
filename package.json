{
  "name": "images-to-gif",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest --watch",
    "e2e-cli": "npx playwright test",
    "e2e": "npx playwright test --ui",
    "build": "npm run copy-ffmpeg && vite build",
    "build-preview": "npm run copy-ffmpeg && vite build --mode preview && npm run preview",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.check.json",
    "copy-ffmpeg": "mkdir -p ./public/lib/ffmpeg/core-st/dist && cp ./node_modules/@ffmpeg/core/dist/esm/* ./public/lib/ffmpeg/core-st/dist",
    "deploy": "sh deploy.sh > deploy.log 2>&1"
  },
  "devDependencies": {
    "@playpilot/svelte-htm": "^1.0.1",
    "@playwright/test": "^1.50.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^4.2.3",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20.17.19",
    "autoprefixer": "^10.4.20",
    "htm": "^3.1.1",
    "jsdom": "^24.1.3",
    "lodash": "^4.17.21",
    "postcss": "^8.5.3",
    "prettier": "^3.5.1",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.85.0",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "svelte-spa-router": "^4.0.1",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "vite": "5.4.14",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^1.6.1"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.12.10",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2"
  }
}
